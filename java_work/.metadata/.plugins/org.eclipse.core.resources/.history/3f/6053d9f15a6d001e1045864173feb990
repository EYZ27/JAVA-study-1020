package org.comstudy.day02.study;

import java.util.Scanner;

public class Study {
	
	static Scanner scan = new Scanner(System.in);
	
	
	public static void main(String[] args) {
		
		Integer score = null;
		System.out.print("점수를 입력하세요. :");
		score = scan.nextInt();
		
		System.out.println(score);
		
	}
	
}
